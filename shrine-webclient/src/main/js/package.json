{
  "name": "shrine-webclient",
  "version": "1.0.0",
  "description": "SHRINE fork of the i2b2 Webclient",
  "main": "index.js",
  "scripts": {
    "build-cell": "cd shrine-cell && npm run build",
    "build-i2b2": "ncp i2b2-src i2b2",
    "clean": "rimraf i2b2/js-i2b2/cells/SHRINE/dist && rimraf i2b2/js-shrine/dist",
    "build-plugin": "cd shrine-plugin && gulp export",
    "build": "npm run build-i2b2 && npm run build-cell && npm run build-plugin",
    "start": "npm run maven-build && gulp serve",
    "build-prod": "npm run clean && npm run build",
    "maven-build": "cd ../../../ && mvn clean install"
  },
  "repository": {
    "type": "git",
    "url": "https://open.med.harvard.edu/stash/scm/shrine/shrine.git"
  },
  "keywords": [
    "Harvard",
    "Medical",
    "School",
    "HMS",
    "SHRINE",
    "I2B2"
  ],
  "author": "Ben Carmen",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "concurrently": "^3.5.0",
    "copyfiles": "^1.2.0",
    "express": "^4.14.0",
    "gulp": "^3.9.1",
    "gulp-server-livereload": "^1.9.2",
    "maven": "^4.2.0",
    "webpack": "^3.4.1"
  },
  "dependencies": {
    "mv": "^2.1.1",
    "request": "^2.79.0",
    "rimraf": "^2.6.2"
  }
}
