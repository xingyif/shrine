<?xml version="1.0" encoding="UTF-8"?>
<web-app>
	 <servlet>
	     <servlet-name>ProxyServlet</servlet-name>
	     <servlet-class>net.shrine.proxy.ShrineProxyServlet</servlet-class>
	     <load-on-startup>1</load-on-startup>
    </servlet>

    <servlet-mapping>
        <servlet-name>ProxyServlet</servlet-name>
        <url-pattern>/request</url-pattern>
    </servlet-mapping>
</web-app>